test <- function(fracB, testFUN, output=c("statistic")) {
  states <- getStates(fracB)
  us <- na.omit(getUniqueStates(fracB))
  testFUN(getSignals(fracB)[states==us[1]], getSignals(fracB)[states==us[2]])[[output]]
}
