  file = {BengtssonHossjer_2006-Methodological study of affine transformations.pdf:BengtssonHossjer_2006-Methodological study of affine transformations.pdf:PDF},
  keywords = {Algorithms; Computational Biology; Data Interpretation, Statistical;
	Gene Expression Profiling; Humans; Image Processing, Computer-Assisted;
	Likelihood Functions; Models, Statistical; Models, Theoretical; Oligonucleotide
	Array Sequence Analysis; Regression Analysis; Software},
  owner = {hb},
  pii = {1471-2105-7-100},
  pmid = {16509971},
  timestamp = {2008.02.06},
  url = {http://dx.doi.org/10.1186/1471-2105-7-100}
