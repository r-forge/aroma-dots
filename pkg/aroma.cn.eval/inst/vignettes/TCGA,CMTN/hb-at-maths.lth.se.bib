@ARTICLE{Edlundh-RoseE_etal_2005,
  author = {Esther Edlundh-Rose and Ilya Kupershmidt and Anna C Gustafsson and
	Tiziana Parasassi and Annalucia Serafino and Luisa Bracci-Laudiero
	and Giulia Greco and Ewa K Krasnowska and Maria-Concetta Romano and
