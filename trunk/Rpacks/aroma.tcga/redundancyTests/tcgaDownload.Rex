if (interactive()) savehistory();library("aroma.tcga");library("R.menu");sites <- list(  "broad.mit.edu_OV.Genome_Wide_SNP_6" = list(    urlRoot = "tcga4yeo",    path = "tumor/ov/cgcc/broad.mit.edu/genome_wide_snp_6/snp",    dataSetPrefix = "broad.mit.edu_OV.Genome_Wide_SNP_6",    archives = c("9.5.0", "11.5.0", "12.6.0", "13.6.0", "14.7.0"),    patterns = c("[.]birdseed[.]data[.]txt$",                 "[.]ismpolish[.]data[.]txt$",                 "[.]after_5NN[.]copynumber[.]data[.]txt$",                 "[.]copynumber[.]byallele[.]data[.]txt$",                 "[.]CEL$")  ),  "hudsonalpha.org_OV.Human1MDuo" = list(    urlRoot = "tcga4yeo",    path = "tumor/ov/cgcc/hudsonalpha.org/human1mduo/snp",    dataSetPrefix = "hudsonalpha.org_OV.Human1MDuo",    archives = c("1.1.0", "2.1.0"),    patterns = c("[.]XandYintensity[.]txt$",                 "[.]Paired_LogR[.]txt$",                 "[.]Genotypes[.]txt$")  ),  "hms.harvard.edu_OV.HG-CGH-244A" = list(    urlRoot = "anonymous",    path = "tumor/ov/cgcc/hms.harvard.edu/hg-cgh-244a/cna",    dataSetPrefix = "hms.harvard.edu_OV.HG-CGH-244A",    archives = c("1.0.0", "2.2.0", "3.0.0", "4.2.0", "5.1.0", "6.0.0"),    patterns = c("[.]data[.]txt$")  ),  "mskcc.org_OV.HG-CGH-244A" = list(    urlRoot = "anonymous",    path = "tumor/ov/cgcc/mskcc.org/hg-cgh-244a/cna",    dataSetPrefix = "mskcc.org_OV.HG-CGH-244A",    archives = c("9.3.0"),    patterns = c("[.]data[.]txt$")  ),  "mskcc.org_OV.CGH-1x1M_G4447A" = list(    urlRoot = "anonymous",    path = "tumor/ov/cgcc/mskcc.org/cgh-1x1m_g4447a/cna",    dataSetPrefix = "mskcc.org_OV.CGH-1x1M_G4447A",    archives = c("11.1.0", "12.1.0", "13.1.0"),    patterns = c("[.]data[.]txt$")  ));site <- sites[[textMenu(names(sites))]];archive <- textMenu(site$archives, value=TRUE);dataSet <- sprintf("%s.%s", site$dataSetPrefix, archive);urlPath <- file.path(site$urlRoot, site$path, dataSet);tdd <- TcgaDccDownloader(urlPath);setUsername(tdd, Sys.getenv("TCGAUSER"));setPassword(tdd, Sys.getenv("TCGAPWD"));print(tdd);filenames <- lapply(site$patterns, FUN=function(pattern) {  listFiles(tdd, pattern=pattern);});n <- sapply(filenames, FUN=length);names(filenames) <- sprintf("%s (n=%d)", site$patterns, n);filenames <- filenames[[textMenu(names(filenames))]];print(filenames);pathnames <- sapply(filenames, FUN=function(filename) {  pathname <- downloadFile(tdd, filename);  print(pathname);  pathname;});print(pathnames);############################################################################# HISTORY:# 2009-05-21# o Completely rewritten with R.menu.# 2009-04-07# o Created.############################################################################